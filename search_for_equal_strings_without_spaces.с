#include <stdio.h>

void copy(char s1[100], char s2[100]);
int comparison(char s1[100], char s2[100]);
void delete_space(char s[100]);
int getlin(char s[100]);


int main() {
        
}

int getlin(char s[100])
{
    int c = ' ';
    int len = 0;
    
    while ((c = getchar()) != EOF && c != '\n' && c != '|') 
    {
        s[len] = c;
        len++;
    }
    
    s[len] = '\0';
    
    return len;
}

void copy(char s1[100], char s2[100])
{
    int i = 0;
    
    while (s1[i] != '\0')
    {
        s2[i] = s1[i];
        i++;
    }
    
    s2[i] = '\0';
}

int comparison(char s1[100], char s2[100])
{
    int check = 1;
    int i = 0;
    
    while (s1[i] != '\0')
    {
        if (s1[i] != s2[i]) 
        {
            check = 0;
            break;
        }
        
        i++;
    }
    
    if (s1[i] != s2[i]) check = 0;
    
    return check;
}

void delete_space(char s[100])
{
    char s1[100];
    int i = 0;
    int j = 0;
    while (s[i] != '\0')
    {
        if (s[i] != ' ')
        {
            s1[j] = s[i];
            j++;
        }
        i++;
    }
    s1[j] = '\0';
    copy(s1, s);
}
